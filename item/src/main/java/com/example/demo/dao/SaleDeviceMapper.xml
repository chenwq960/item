  <select id="selectList" resultMap="BaseResultMap">
	  select sd.*,st.*,d.*,d.*,u.* from tb_sale_device sd
	left join tb_station st on st.station_id = sd.station_id
	left join tb_device d on d.device_id = sd.device_id
	left join tb_user u on u.user_id = sd.sale_user
	where sd.is_delete = 0
	<if test="searchKey != null and searchKey != ''">
		and d.device_name like concat('%',#{searchKey},'%')
	</if>
	<if test="startTime != null and startTime != ''">
		and sd.sale_number = #{startTime}
		or sd.sale_time like concat('%',#{startTime},'%')
	</if>
  </select>
